<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/FrameLayoutuserhome"
        android:layout_above="@id/bottomnevigatiomuserhome"
        />
    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:layout_width="match_parent"
        android:layout_height="108dp"
        android:id="@+id/bottomnevigatiomuserhome"
        app:menu="@menu/needybottom"
        android:layout_alignParentBottom="true"
        app:itemRippleColor="@color/blue"
        app:itemIconTint="#BFC1F3"
        app:itemActiveIndicatorStyle="@style/App.Custom.Indicator"
        app:itemTextColor="#888686"
        android:background="@color/white"
        />


</RelativeLayout>